{
  "manifest_version": 2,

  "name": "Instant Bootstrap",
  "version": "0.0.1",
  "description": "Customize Bootstrap 3 on your page instantly!",
  "options_page": "panel/index.html",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts.js"]
    }
  ],

  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}